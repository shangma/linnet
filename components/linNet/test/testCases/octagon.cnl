R R     k1  k2
R R02   k2  k3  R02=R
R R03   k3  k4  R03=R
R R04   k4  k5  R04=R
R R05   k5  k6  R05=R
R R06   k6  k7  R06=R
R R07   k7  k8  R07=R
R R08   k8  k1  R08=R

R R09   k1  k9  R09=R
R R10   k2  k9  R10=R
R R11   k3  k9  R11=R
R R12   k4  k9  R12=R
R R13   k5  k9  R13=R
R R14   k6  k9  R14=R
R R15   k7  k9  R15=R
R R16   k8  k9  R16=R

/* The input voltage between top and bottom. */
U    U1 k1  k5

/* The voltage at the middle of the octagon needs to be half the input
   voltage due to the symmetry. */
DEF  U2 k9  k5
PLOT G  U2  U1

/* The input impedance is computed, which is the wanted overall resistance
   of the octagon. Can be figured out as 6/7*R due to the symmetry of the
   circuit. */
PLOT Z  U1  I_U1
