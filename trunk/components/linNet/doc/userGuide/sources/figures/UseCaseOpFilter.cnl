// Definition of in- and output
U   Uin  Ip  gnd
DEF Uout out gnd

// The circuit
OP OP  Ip  Im  out
R  R1  out Im  R1=1k
C  C1  out Im  C1=1u
R  R2  Im  gnd R2=1k
C  C2  Im  gnd C2=22u

// Definition of results
PLOT G    Uout Uin
RES  Gf   Uout
RES  SIMO Uout U_Im I_OP
PLOT Iop  I_OP Uin